{
    "name": "krokedil/ledyer-payments-for-woocommerce",
    "description": "",
    "version": "1.0.0",
    "type": "wordpress-plugin",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": ">=7.4",
        "krokedil/wp-api": "^1.0.0",
        "krokedil/woocommerce": "^1.0.0"
    },
    "require-dev": {
        "php-stubs/woocommerce-stubs": "^8.1",
        "wp-coding-standards/wpcs": "^3.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Krokedil\\Ledyer\\Payments\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "wpify/scoper": true
        }
    },
    "extra": {
        "wpify-scoper": {
            "prefix": "KrokedilLedyerPaymentsDeps",
            "folder": "dependencies",
            "autorun": true,
            "globals": [
                "wordpress",
                "woocommerce"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:krokedil/wp-api.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:krokedil/woocommerce.git"
        }
    ],
    "minimum-stability": "dev"
}
