{
  "name": "ledyer-payments-for-woocommerce",
  "version": "1.0.0",
  "description": "Ledyer Payments for WooCommerce",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "pnpx wp-prettier src/assets/js --paren-spacing --tab-width 4 --print-width 120 --no-semi --write",
    "makepot": "wp-pot --src='**/*.php' --dest-file='languages/ledyer-payments-for-woocommerce.pot' --domain='ledyer-payments-for-woocommerce' --package='Ledyer Payments for WooCommerce'",
    "dev": "vite",
    "build": "pnpm makepot && pnpm lint && vite build"
  },
  "keywords": [],
  "author": "Ledyer/Krokedil",
  "license": "ISC",
  "devDependencies": {
    "@wordpress/eslint-plugin": "20.1.0",
    "prettier": "npm:wp-prettier@3.0.3",
    "terser": "5.29.2",
    "vite": "5.2.6",
    "wp-pot-cli": "1.5.0",
    "wp-prettier": "3.0.3"
  }
}
